{
  "metadata": [
    {
      "noRestore": true,
      "src": [
        {
          "src": "{{sourceCodeDirectory}}",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "{{outputDirectory}}",
    "template": [
      "statictoc",
      "../_exported_templates/override"
    ],
    "globalMetadata": {
      "_appName": "{{appName}}",
      "_appTitle": "{{appName}}",
      "_appFooter": "<pre></pre>"
    }
  }
}